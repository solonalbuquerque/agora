  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "migrate": "node scripts/migrate.js",
    "migrate:start": "node scripts/migrate.js && node src/server.js",
    "migrate:dev": "node scripts/migrate.js && nodemon src/server.js",
    "seed": "node scripts/seed.js",
    "sign-request": "node scripts/sign-request.js",
    "sign-issuer-request": "node scripts/sign-issuer-request.js",
    "promote-trust-levels": "node scripts/promote-trust-levels.js",
    "docker:down": "docker compose down",
    "docker:reset": "docker compose down -v",
    "docker:dev": "docker compose -f docker-compose.yml -f docker-compose.dev.yml up --build",
    "docker:up": "docker compose up -d --build",
    "build:staff": "cd staff-ui && npm install && npm run build"
  },
  "dependencies": {
    "@fastify/cookie": "^9.3.1",
